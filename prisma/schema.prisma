generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model HairTransplantForm {
  id             Int      @id @default(autoincrement())
  firstName      String
  lastName       String
  email          String
  contact        String
  gender         String
  transplantType String
  photos         Json
  createdAt      DateTime @default(now())
}
